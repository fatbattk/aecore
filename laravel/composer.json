{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"illuminate/html": "~5.0",
    "illuminate/support": "4.*|5.*",
    "aws/aws-sdk-php-laravel": "~2.0",
    "monarobase/country-list": "dev-master",
		"intervention/image": "~2.1",
		"intervention/imagecache": "~2.1",
		"nesbot/carbon": "~1.18",
		"morrislaptop/laravel-five-package-bridges": "~1.0",
    "camroncade/timezone": "dev-master",
		"barryvdh/laravel-debugbar": "~2.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database",
      "app/Models"
		],
		"psr-4": {
			"App\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
